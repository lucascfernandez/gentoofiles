                    <script type="text/javascript">

                        function jumpToPage(articleId,pageId, sectionId, path) {
                            if (typeof path == 'undefined') path = "2022/02/22" ;
                            var zoom_page = '#zoom=page-width';

                            $.ajax({
                              url: "/Ranking?action=registerPV&publication=lasegunda&date="+path+"&articleId="+articleId+"&section="+sectionId
                            }).done(function() {
                              console.log("PV registered");
                              if (0) {
                              pagesData.forEach(function(item,index) {
                                            console.log(index);
                                            console.log(item);      
                                            if (item.id==pageId) {
                                                    var slider = $(".royalSlider").data('royalSlider');
                                                    slider.goTo(index-1);   
                                            } 
                                    });
                            } else {
                                    window.location="/"+path+"/"+sectionId+"/"+pageId + zoom_page;
                            }
                            });
                            return false; 
                        }
                     
                    </script>
                 

      

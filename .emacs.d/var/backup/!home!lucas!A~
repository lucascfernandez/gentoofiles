      $('#modal_lock_tools').modal();
    } else {
      if ($('.cont_buscador_mer').is(':hidden')) {
          $('.cont_buscador_mer').slideDown("slow");
      } else {
          $('.cont_buscador_mer').slideUp("slow");
          ShowNewspaperPage();
          setTimeout(function(){ EmptySearch(); }, 2000);
      }
    }

  });

  if (pramEncodedData) {
    componentHandler.upgradeAllRegistered();
  }


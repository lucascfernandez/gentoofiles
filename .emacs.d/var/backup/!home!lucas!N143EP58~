   <script>

  	var linkArticle = [];
  	function addMultimedia(articleId, embed) {
  		if (multimediaArticle[articleId]) {
  			multimediaArticle[articleId].push(embed);	
  		} else {
  			multimediaArticle[articleId] = [];
  			multimediaArticle[articleId].push(embed);	
  		}
  	}
  	function addLink(articleId, link) {
  		if (linkArticle[articleId]) {
  			linkArticle[articleId].push(link);	
  		} else {
  			linkArticle[articleId] = [];
  			linkArticle[articleId].push(link);	
  		}

  	}
	
                  

                  

        
                  

                  

        
                  

                  

        
                  

                  

        
                  

                  

        
                  

                  

        
   </script> 

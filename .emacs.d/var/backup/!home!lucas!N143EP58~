            $('.cont_log_full_msj .cont_login_msj').html('El usuario ingresado no posee una suscripción de La Segunda Digital.</br>Lo invitamos a ser parte de esta nueva experiencia haciendo clic en el bóton <strong>"Suscríbete"</strong>.');

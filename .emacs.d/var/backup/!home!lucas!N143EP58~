      $(".bookmarks_mer").click(function () {

        if($(".cont_fav_mer").hasClass("open")) { 
          $('.cont_fav_mer').removeClass('open');
          $('.close_html').show();
          
        } else {
          $('.cont_fav_mer').addClass('open');
          $('.close_html').hide();

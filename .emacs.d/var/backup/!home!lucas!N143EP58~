       (
           function (a,b,c,d) {
             b.type= c;
             b.src= d;
             a.appendChild(b);
       })
       (document.getElementsByTagName('head')[0], document.createElement('script'), 'text/javascript', '/assets/pdf/build/pdf.js');
     }
